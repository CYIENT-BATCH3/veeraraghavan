double pointer examples.
#include <stdio.h>

int main() {
    int a=10;
    int *p1,**p2,***p3;

    p1=&a;
    p2=&p1;
    p3=&p2;
    printf("\n address of a %d",p1);
    printf("\n  value of a %d",*p1);
    printf("\n address of p1 %d",p2);
    printf("\n value of p1 %d",*p2);
    printf("\n address of p2 %u",p3);
    printf("\n value of p2 %u",*p3);
    
}
